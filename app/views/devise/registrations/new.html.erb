
 <div class="content-wrapper", style='width: 100%';>
    <h3 class="text-primary mb-4"></h3>
    <div class="row mb-2", style='width: 100%;'>
        <div class="col-lg-12">
            <div class="card">
                <div class="card-block", style='width: 800px;'>
                    <h4 class="card-title mb-4">Registrar Cuenta</h4>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.label :Correo %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :Correo %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> Caracteres minimos)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label :Confirmar_pasword %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit "Aceptar", :class=>"btn btn-primary" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
