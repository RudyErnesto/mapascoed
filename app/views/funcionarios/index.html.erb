<p id="notice"><%= notice %></p>
<div class="content-wrapper">
  <h3 class="text-primary mb-4">funcionarios</h3>
  <div class="row mb-2">
      <div class="col-lg-12">

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Celular</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th>Ci</th>
      <th>Funcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funcionarios.each do |funcionario| %>
      <tr>
        <td><%= funcionario.nombre %></td>
        <td><%= funcionario.apellido %></td>
        <td><%= funcionario.celular %></td>
        <td><%= funcionario.telefono %></td>
        <td><%= funcionario.correo %></td>
        <td><%= funcionario.ci %></td>
        <td><%= funcionario.tipo %></td>
        <td><%= link_to 'Mostrar', funcionario, :class => 'btn btn-primary'%></td>
        <td><%= link_to 'Editar', edit_funcionario_path(funcionario), :class => 'btn btn-warning'%></td>
        <td><%= link_to 'Eliminar', funcionario, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-danger'%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Funcionario', new_funcionario_path , :class => 'btn btn-success'%>
<div style='width: 800px;'>
  <div id="map" style='width: 800px; height: 400px;'></div>
</div>
