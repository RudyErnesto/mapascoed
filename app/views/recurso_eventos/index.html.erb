<p id="notice"><%= notice %></p>
<div class="content-wrapper">
  <h3 class="text-primary mb-4">Recursos usados</h3>
  <div class="row mb-2">
      <div class="col-lg-12">


<table>
  <thead>
    <tr>

      <th>Fechapedido</th>

      <th>Asignadoa</th>
      <th>Desmovilizadopor</th>
      <th>Fechadesmovilizacion</th>
      <th>Observaciones</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recurso_eventos.each do |recurso_evento| %>
      <tr>

        <td><%= recurso_evento.fechapedido %></td>

        <td><%= recurso_evento.asignadoa %></td>
        <td><%= recurso_evento.desmovilizadopor %></td>
        <td><%= recurso_evento.fechadesmovilizacion %></td>
        <td><%= recurso_evento.observaciones %></td>
        <td><%= link_to 'Mostrar', recurso_evento, class: "btn btn-primary" %></td>
        <td><%= link_to 'Editar', edit_recurso_evento_path(recurso_evento), class: "btn btn-warning" %></td>
        <td><%= link_to 'Eliminar', recurso_evento, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Recurso Usado', new_recurso_evento_path, class: "btn btn-success"  %>
