<p id="notice"><%= notice %></p>
<div class="content-wrapper">
  <h3 class="text-primary mb-4">Recursos usados</h3>
  <div class="row mb-2">
      <div class="col-lg-12">


<table>
  <thead>
    <tr>
      <th>Solicitado</th>
      <th>Fechapedido</th>
      <th>Clase</th>
      <th>Tipo</th>
      <th>Fechallegada</th>
      <th>Institucion</th>
      <th>Matricula</th>
      <th>Numeropersonas</th>
      <th>Estado</th>
      <th>Asignadoa</th>
      <th>Desmovilizadopor</th>
      <th>Fechadesmovilizacion</th>
      <th>Observaciones</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recurso_eventos.each do |recurso_evento| %>
      <tr>
        <td><%= recurso_evento.solicitado %></td>
        <td><%= recurso_evento.fechapedido %></td>
        <td><%= recurso_evento.clase %></td>
        <td><%= recurso_evento.tipo %></td>
        <td><%= recurso_evento.fechallegada %></td>
        <td><%= recurso_evento.institucion %></td>
        <td><%= recurso_evento.matricula %></td>
        <td><%= recurso_evento.numeropersonas %></td>
        <td><%= recurso_evento.estado %></td>
        <td><%= recurso_evento.asignadoa %></td>
        <td><%= recurso_evento.desmovilizadopor %></td>
        <td><%= recurso_evento.fechadesmovilizacion %></td>
        <td><%= recurso_evento.observaciones %></td>
        <td><%= link_to 'Show', recurso_evento, class: "btn btn-primary" %></td>
        <td><%= link_to 'Edit', edit_recurso_evento_path(recurso_evento), class: "btn btn-warning" %></td>
        <td><%= link_to 'Destroy', recurso_evento, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Recurso Usado', new_recurso_evento_path, class: "btn btn-success"  %>
